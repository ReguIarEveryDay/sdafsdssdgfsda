CloseOpen.TextScaled = true
CloseOpen.TextSize = 21.000
CloseOpen.TextWrapped = true
CloseOpen.Draggable = true
CloseOpen.Active = true
CloseOpen.MouseButton1Down:connect(function()
	if Main.Visible == true then
		Main.Visible = false
	else
		Main.Visible = true
	end
end)


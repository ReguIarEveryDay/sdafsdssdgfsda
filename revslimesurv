SkillsArrows.Name = "Skills/Arrows"
SkillsArrows.Parent = Main
SkillsArrows.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
SkillsArrows.BackgroundTransparency = 0.400
SkillsArrows.BorderColor3 = Color3.fromRGB(0, 0, 0)
SkillsArrows.BorderSizePixel = 5
SkillsArrows.Position = UDim2.new(0.35, 0, 0.39641434, 0)
SkillsArrows.Size = UDim2.new(0, 200, 0, 50)
SkillsArrows.Font = Enum.Font.RobotoMono
SkillsArrows.Text = "Give Skills/Arrows"
SkillsArrows.TextColor3 = Color3.fromRGB(0, 0, 0)
SkillsArrows.TextSize = 25.000
SkillsArrows.TextWrapped = true
SkillsArrows.MouseButton1Down:connect(function()
	local plr = game:GetService("Players").LocalPlayer
    
    plr.Skill1.Value = true
    plr.Skill2.Value = true
    plr.Skill3.Value = true
    plr.Skill4.Value = true
    plr.Skill5.Value = true
    plr.Skill6.Value = true
    plr.Skill7.Value = true
    plr.Skill8.Value = true
    plr["Arrow 2"].Value = true
    plr["Arrow 3"].Value = true
    plr["Arrow 4"].Value = true
    plr["Arrow 5"].Value = true
    plr["Arrow 6"].Value = true
    plr["Arrow 7"].Value = true
    plr["Arrow 8"].Value = true
    plr["Arrow 9"].Value = true
    plr["Arrow 10"].Value = true
    plr["Arrow 11"].Value = true
    end)

